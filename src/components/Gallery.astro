---
import Card from "../components/Card.astro";

import DATA from "../data.js";
---

<ul class="grid grid-cols-2 lg:grid-cols-4 gap-12">
  {
    DATA.map((item) => (
      <li id="item.id">
        <Card
          id={item.id}
          title={item.title}
          subtitle={item.subtitle}
          price={item.price}
          image={item.image}
        />
      </li>
    ))
  }
</ul>
